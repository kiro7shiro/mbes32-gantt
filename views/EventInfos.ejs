<div id="<%= id %>" class="event-infos">
    <header class="w3-container w3-theme">
        <h1><%= id.replaceAll('_', ' ') %></h1>
    </header>
    <div class="w3-container w3-theme-l5">
        <p><%= name %></p>
        <p>Begin: <%= start.toLocaleDateString() %></p>
        <p>End: <%= end.toLocaleDateString() %></p>
        <p>Hallen:</p>
        <p><%= halls.join(', ') %></p>
    </div>
    <footer class="w3-container w3-theme">&nbsp;</footer>
</div>
